{
  "name": "@nvivn/base",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "bin": {
    "nvivn-dev": "dev.js",
    "nvivn-app-server": "server.js"
  },
  "scripts": {
    "start": "budo index.js:base.js --pushstate -- -t browserify-css",
    "build:base": "browserify -t browserify-css index.js -o dist/base.js",
    "watch:base": "watchify -t browserify-css index.js -o dist/base.js -v",
    "build:base:minified": "browserify -t browserify-css index.js -o dist/base.js -g uglifyify",
    "build:client": "browserify client.js -o dist/client.js -g uglifyify",
    "build:html": "cp index-prod.html dist/index.html && cp dev/iframe.html dist/",
    "build": "npm run build:base:minified && npm run build:client && npm run build:html && npm run copy:worker",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Jesse Kriss <jesse@jklabs.net>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.2.2",
    "babelify": "^10.0.0",
    "browserify": "^16.2.3",
    "browserify-css": "^0.14.0",
    "budo": "^11.5.0",
    "choo": "^6.13.1",
    "fs-extra": "^7.0.1",
    "gifencoder": "^2.0.1",
    "instascan": "git+https://github.com/jkriss/instascan.git",
    "postmsg-rpc": "^2.4.0",
    "proquint": "0.0.1",
    "qrcode": "^1.3.2",
    "qrloop": "^0.8.1",
    "st": "^1.2.2",
    "terser": "^3.14.1",
    "uglifyify": "^5.0.1"
  },
  "browserify-css": {
    "autoInject": true,
    "minify": true,
    "rootDir": "."
  },
  "devDependencies": {
    "watchify": "^3.11.0"
  }
}
